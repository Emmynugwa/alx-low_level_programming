#include <stdio.h>

/**
 * main - Prints all possible combinations of single-digit numbers
 *
 * Description: Prints all possible combinations of single-digit numbers,
 * separated by ", " and in ascending order
 *
 * Return: 0 (Success)
 */
int main(void)
{
	int number;

	for (number = 0; number <= 9; number++)
	{
		putchar(number + '0');

		if (number != 9)
		{
			putchar(',');
			putchar(' ');
		}
	}

	putchar('\n');

	return 0;
}
